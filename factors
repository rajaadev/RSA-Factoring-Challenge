#!/usr/bin/bash

FILE = $1

while read NUMBER;
do
	FACTORS=($(factor $NUMBER))
	# Extract the factors from the output of 'factor' command
	FACTOR1=${FACTORS[1]}
	FACTOR2=${FACTORS[2]}
	# Calculate the product of the two factors
	PRODUCT=$((NUMBER / FACTOR1 * FACTOR2))
	# Print the factorization in the required format
	echo "$NUMBER=$FACTOR1*$FACTOR2"
done < $FILE
